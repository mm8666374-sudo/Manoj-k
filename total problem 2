14.ProductOfDigits.java
import java.util.Scanner;
public class ProductOfDigits {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();
		int prod =1, digit;
		while(n>0) {
			digit = n % 10;
			prod *= digit;
			n /= 10;					
		}
		System.out.println(prod);
		sc.close();
	}
}
15.ReverseaNumber.java
import java.util.Scanner;
public class ReverseaNumber {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt(), digit, rev=0;
		while(n>0) {
			digit = n %10;
			rev = rev*10 + digit;
			n /=10;
		}
		System.out.println(rev);
		sc.close();
	}
}
16.SpyNumber.java
import java.util.Scanner;
public class SpyNumber {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt(), digit, sum=0, prod =1;;
		while(n>0) {
			digit = n %10;
			sum += digit;
			prod *= digit;
			n /=10;
		}
		if(sum == prod) System.out.println("Spy Number");
		else System.out.println("Not Spy Number");
		sc.close();
	}
}
17.StrongNumber.java
import java.util.Scanner;
public class StrongNumber {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();
		int count=0, temp1=n, temp2=n, fact=1, digit, sum =0;
		while(n >0) {
			count++;
			n/=10;
		}
		for(int i=0; i<count; i++) {
			digit = temp1 % 10;
			for(int j=digit; j>0; j--) {
				fact *= j;
			}
			temp1 /=10;
			sum += fact;
			fact =1;
		}
		if(sum == temp2) System.out.println("Strong Number");
		else System.out.println("Not Strong Number");
		sc.close();
	}
}
