19.SumOfEvenDigits.java
import java.util.Scanner;
public class SumOfEvenDigits {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();
		int sum =0, digit;
		while(n >0) {
			digit = n % 10;
			if(digit %2 ==0) sum+= digit;
			n /= 10;
		}
		System.out.println(sum);
		sc.close();
	}
}
20.SwapFirstAndLastDigit.java
import java.util.Scanner;
public class SwapFirstAndLastDigit {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();
		int count=0, temp=n;
		while(n>0) {
			count++;
			n /= 10;
		}
		int arr[] = new int[count];
		String name = Integer.toString(temp);
		for(int i=0; i<count; i++) {
			arr[i] = name.charAt(i);
		}
		int temp2 = arr[0];
		arr[0] = arr[count-1];
		arr[count-1] = temp2;
		for(int a:arr) System.out.print((char)a);
		sc.nextInt();
	}
}
